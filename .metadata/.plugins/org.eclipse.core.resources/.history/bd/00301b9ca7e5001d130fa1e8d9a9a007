package OOPS;
class Shape{
	public void shapeCall() {
		System.out.println("shape class");
	}
	public void run() {
		System.out.println("run");
	}
}
class Circle extends Shape{
	public void shapeCall() {
		System.out.println("circle shape class");
	}
}
public class Overriding {
	public static void main(String[] args) {
		Circle c = new Circle();
		c.shapeCall();
		c.shapeCall();
		c.run();
	}
}
